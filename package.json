{
  "name": "model-api-ts",
  "version": "0.0.0",
  "main": "src/server.ts",
  "author": "Talisson Maciel Luques - TML Desenvolvimento de Software Ltda.",
  "license": "MIT",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=.env.dev nodemon -r dotenv/config src/server.ts",
    "qa": "DOTENV_CONFIG_PATH=.env.qa nodemon -r dotenv/config src/server.ts",
    "start": "node dist/server.js ",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "prisma": "npx prisma generate",
    "db:create": "npx prisma migrate dev --name init",
    "db:migrate": "npx prisma migrate dev --name $MIGRATION",
    "lint": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@prisma/client": "^4.13.0",
    "@types/nodemailer": "^6.4.9",
    "axios": "^1.4.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "crypto-js": "^4.1.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^11.6.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.4",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.14",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@rocketseat/eslint-config": "^1.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^20.2.0",
    "@types/uuid": "^9.0.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "nodemon": "^3.0.1",
    "prisma": "^4.13.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.9.0",
    "tsx": "^3.12.7",
    "typescript": "^4.1.3"
  }
